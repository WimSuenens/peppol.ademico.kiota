<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo> <!-- setting GenerateAssemblyInfo to false causes this bug https://github.com/dotnet/project-system/issues/3934 -->
    <TargetFrameworks>net9.0;net8.0</TargetFrameworks>
    <AssemblyName>Peppol.Ademico.Kiota</AssemblyName>
    <PackageId>Peppol.Ademico.Kiota</PackageId>
    <OutputType>Library</OutputType>
    <Authors>WimSuenens</Authors>
    <Company>WimSuenens</Company>
    <AssemblyTitle>Peppol C# SDK Library</AssemblyTitle>
    <Description>A C# SDK to access the Peppol network, build on top of the Ademico openapi and generated by Kiota.</Description>
    <Copyright>No Copyright</Copyright>
    <RootNamespace>Peppol.Ademico.Kiota</RootNamespace>
    <Version>0.0.1</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Peppol.Ademico.Kiota.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/WimSuenens/peppol.ademico.kiota</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>Minor update</PackageReleaseNotes>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ManagePackageVersionsCentrally>false</ManagePackageVersionsCentrally>
    <AssemblyVersion>0.0.1</AssemblyVersion>
    <FileVersion>0.0.1</FileVersion>
  </PropertyGroup>
  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../../README.md" Pack="true" PackagePath="\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.9" />
    <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="9.0.9" />
    <PackageReference Include="Microsoft.Kiota.Bundle" Version="1.19.1" />
    <PackageReference Include="OneOf" Version="3.0.271" />
  </ItemGroup>
</Project>